      "claude",
      "cursor"
    ]
  },
  "footer": {
    "socials": {
      "website": "https://rapportapi.com",
      "linkedin": "https://linkedin.com/company/sidequestinc"
    }
  },
  "api": {
    "baseUrl": "https://api.rapportapi.com",
    "auth": {
      "method": "bearer",
      "name": "Authorization"
    }
  },
  "openapi": "/api-reference/openapi.json"
