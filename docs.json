  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "authentication"
            ]
          },
          {
            "group": "Features",
            "pages": [
              "webhooks",
              "credits"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Research",
            "pages": [
              "api-reference/endpoint/submit-research",
              "api-reference/endpoint/get-research"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Playground",
          "href": "https://rapportapi.com/home/playground",
          "icon": "play"
        },
        {
          "anchor": "Dashboard",
          "href": "https://rapportapi.com/home",
          "icon": "gauge"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@rapportapi.com"
      }
